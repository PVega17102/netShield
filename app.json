{
  "expo": {
    "name": "netShield",
    "slug": "net-shield",
    "version": "1.0.0",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "css"]
    },
    "orientation": "portrait",
    "icon": "./assets/netShield.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/netShield.png",
      "resizeMode": "contain",
      "backgroundColor": "#f4f5f6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/netShield.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/netShield.png"
    },
    "extra": {
      "eas": {
      }
    },
    "owner": "paulvs17"
  }
}
